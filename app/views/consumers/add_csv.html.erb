  <div class="row">
        
    <div class="span12">          
          
          <div class="widget stacked ">
            
            <div class="widget-header">
              <i class="icon-pencil"></i>
              <h3>Import Text File</h3>
          </div> <!-- /widget-header -->
        
             <div class="widget-content">


<%= form_tag import_consumers_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>



<%= search_form_for @search do |f| %>
  <div class="field">
    <%= f.label :name_cont, "Name" %>
    <%= f.text_field :name_cont %>
    <%= f.label :dtc_cont, "DTC" %>
    <%= f.text_field :dtc_cont %>
     <%= f.label :bu_cont, "Bu" %>
    <%= f.text_field :bu_cont %>
     <%= f.label :pc_cont, "PC" %>
    <%= f.text_field :pc_cont %>
  </div>
  <div class="actions"><%= f.submit "Search" ,:class => "btn btn-primary" %></div>
<% end %>
</div>  
</div>
</div>
</div>
