  <div class="row">
        
    <div class="span12">          
          
          <div class="widget stacked ">
            
            <div class="widget-header">
              <i class="icon-pencil"></i>
              <h3>Listing Mobile Users</h3>
          </div> <!-- /widget-header -->
        
             <div class="widget-content">


<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">

  <tr>
    <th>Imei</th>
    <th>Username</th>
     <th>Action</th>
  </tr>
<% @mobileusers.each do |mobileuser| %>
 <tr>
	<td><%= mobileuser.imei%></td>
   <td> <%= mobileuser.user.username rescue nil %></td>

  <td> 
   <%= link_to 'Edit', edit_mobileuser_path(mobileuser) ,:class=>"btn"%>
      <a href="/mobileusers/<%= mobileuser.id %>/delete" onclick="if(confirm('Are you sure you want to delete this page?'))  return true;  else  return false;" class="btn" >Destroy</a></td>
   </tr>   
<% end %>
</table>
<p><%= link_to "New mobileuser", new_mobileuser_path,:class =>"btn btn-secondary" %></p>
</div>  
</div>
</div>
</div>