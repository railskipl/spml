  <div class="row">
        
    <div class="span12">          
          
          <div class="widget stacked" style="width:104%;">
            
            <div class="widget-header">
              <i class="icon-pencil"></i>
              <h3>Listing Sub Cluster Consumers</h3>
          </div> <!-- /widget-header -->
        
             <div class="widget-content">
             	      <section id="tables">
          
          
          
          <div class="row-fluid">
          <div class="span6">
          <h3>Consumers</h3>
          </div>
  
          <div class="span6" style="text-align:right">
          <a href="<%= mr_consumers_path(params.merge(format: "xls")) %>" class="btn btn-secondary"><img src="/assets/excel.png" alt=""> Excel</a> 
          </div>
          
          
          
          </div>
          
<% if @consumers.empty? %>
<p>Sorry! Consumer not found!</p>
<% else %>
<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-highlight">  <thead>
    <tr>
      <th>Sr. No</th>
      <th>Account Number</th>
      <th>Computer Number</th>
      <th>Consumer Code</th>
      <th>Consumer Number</th>
      <th>Name</th>
      <th>Consumer add1</th>
      <th>Consumer add2</th>
      <th>Meter No</th>
      <th>Sub Cluster</th>
      <th>Cluster Id</th>
      <th>Walking Sequence Number</th>
    </tr>
  </thead>

  <tbody>
   <% @consumers.each.with_index(1) do |consumer,index| %>
      <tr>
      	<td><%= index %></td>
      	<td><%= consumer.account_no %></td>
        <td><%= consumer.computer_no %></td>
        <td><%= consumer.cons_code %></td>
        <td><%= consumer.cons_no %></td>
        <td><%= consumer.name %></td>
        <td><%= consumer.conn_add1 %></td>
        <td><%= consumer.conn_add2 %></td>
        <td><%= consumer.meter_no %></td>
        <td><%= consumer.sub_cluster %></td>
        <td><%= consumer.cluster_id %></td>
        <td><%= consumer.walking_seq_no %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% end %>

</sections>
</div>
</div>
</div>
</div>