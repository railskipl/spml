
<% if params[:consumer_status] == "true" %>

<div class="page-header-custom">
            <h3 align = "center">
               Old consumers Readings</h3>
            <!--subnav-->
          
        </div>

<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">
  
    <tr>
      <th>Sr. No</th>
      <th>Consumer number</th>
      <th>Reader</th>
      <th>Team</th>
      <th>Meter No</th>
      <th>New Meter No</th>
      <th>Meter reading</th>
      <th>Sub Cluster</th>
      <th>Meter Status</th>
      <th>Meter Reader Status</th>
      <th>Date Time</th>
    </tr>
  

 
    <%  @readings_status.each.with_index(1) do |reading,index| %>
      <tr>
        <td><%= index %></td>
        <td><%= reading.consumer_no rescue nil%>&nbsp;</td>
        <td><%= reading.read_by rescue nil%>&nbsp;</td>
        <td><% a =  TeamUser.find_by_user_id(reading.user_id).team_id %>
      <%= User.find(Team.find(a).user_id).username rescue nil %>
        </td>
        <td><%= reading.old_meter_no rescue nil %></td>
        <td><%= reading.new_meter_no rescue nil %></td>
        <td><%= reading.meter_reading  rescue nil%>&nbsp;</td>
        <td><%= reading.sub_cluster rescue nil%>&nbsp;</td>
        <td><%= reading.meter_status rescue nil%>&nbsp;</td>
        <td><%= reading.meter_reader_status rescue nil%>&nbsp;</td>
        <td><%= reading.created_at.strftime("%T %a %b %e %Y ")%></td>
      </tr>
    <% end %>
  
</table>
<% else %>
<div class="page-header-custom">
            <h3 align = "center">
               New consumers Readings</h3>
            <!--subnav-->
          
        </div>

<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">
  
    <tr>
      <th>Sr. No</th>
      <th>Consumer number</th>
      <th>Reader</th>
      <th>Team</th>
      <th>Meter No</th>
      <th>New Meter No</th>
      <th>Meter reading</th>
      <th>Sub Cluster</th>
      <th>Meter Status</th>
      <th>Meter Reader Status</th>
      </tr>
  

 
    <%  @readings_status.each.with_index(1) do |reading,index| %>
      <tr>
        <td><%= index %></td>
        <td><%= reading.consumer_no rescue nil%>&nbsp;</td>
        <td><%= reading.read_by rescue nil%>&nbsp;</td>
        <td><% a =  TeamUser.find_by_user_id(reading.user_id).team_id %>
      <%= User.find(Team.find(a).user_id).username rescue nil %>
        </td>
        <td><%= reading.old_meter_no rescue nil %></td>
        <td><%= reading.new_meter_no rescue nil %></td>
        <td><%= reading.meter_reading  rescue nil%>&nbsp;</td>
        <td><%= reading.sub_cluster rescue nil%>&nbsp;</td>
        <td><%= reading.meter_status rescue nil%>&nbsp;</td>
        <td><%= reading.meter_reader_status rescue nil%>&nbsp;</td>
        <td><%= reading.created_at.strftime("%T %a %b %e %Y ")%></td>
      </tr>
    <% end %>
  
</table>

<% end %>