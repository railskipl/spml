<% if @teams.empty? %>
<p>Sorry! No Vendor there...</p>
<% else %>

<% @teams.each do |team| %>
  <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-highlight">
     
    <tr>
      
      <th>Consumer number</th>
      <th>Reader</th>
      <th>Remarks</th>
      <th>Meter reading</th>
      <th>Dtc</th>
      <th>Meter Status</th>
      <th>Meter Reader Status</th>
      <th>Latitude</th>
      <th>Longitude</th>
    </tr>
  
  <% TeamUser.find_all_by_team_id(team.id).each do |reader|%>
  <% a = Reading.find_all_by_user_id(reader.user_id) rescue nil %>
 <tr>
    <% a.each do |reading | %> <tr>
      <td><%= reading.consumer_no rescue nil%>&nbsp;</td>
      <td><%= reading.user.first_name rescue nil%>&nbsp;</td>
      <td><%= reading.remark rescue nil %></td>
      <td><%= reading.meter_reading.to_i  rescue nil%>&nbsp;</td>
      <td><%= reading.dtc rescue nil%>&nbsp;</td>
      <td><%= reading.meter_status rescue nil%>&nbsp;</td>
      <td><%= reading.meter_reader_status rescue nil%>&nbsp;</td>
      <td><%= reading.latitude rescue nil%>&nbsp;</td>
      <td><%= reading.longitude rescue nil%>&nbsp;</td>
     <% end %>
      </tr>
    <% end %>
 </table>
    <% end %>
   <a href="<%= search_vendor_report_path(params.merge(format: "xls")) %>" class="btn btn-secondary"><img src="/assets/excel.png" alt=""> Excel</a>   

<% end %>

