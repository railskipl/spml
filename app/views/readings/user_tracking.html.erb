      <i class="icon-time"></i>
      </span>
      </div>
    </td>
  </tr>
</table>
 
 <script type="text/javascript">
 $('#timepicker1').timepicker({
minuteStep: 1,
template: 'modal',
appendWidgetTo: 'body',
showSeconds: true,
