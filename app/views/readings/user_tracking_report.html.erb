<% if @readings.empty? %>
No results found!
<% else %>
          <div class="row-fluid">
          <div class="span6">
          <h3>Consumers Readings</h3>
          </div>
  
          <div class="span6" style="text-align:right">
      <a href="<%= search_by_date_path(params.merge(format: "xls")) %>" class="btn btn-secondary"><img src="/assets/excel.png" alt=""> Excel</a> 
  <a href="<%= search_by_date_path(params.merge(format: "pdf")) %>" class="btn btn-secondary"><img src="/assets/pdf.png" alt=""> Pdf</a> 
          </div>
          
          
          
          </div>
<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">
  <thead>
  
    <tr>
      <th></th>
      <th>Consumer number</th>
      <th>Reader</th>
      <th>Team</th>
      <th>Meter reading</th>
      <th>Sub Cluster</th>
      <th>Meter Status</th>
      <th>Meter Reader Status</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Date Time</th>
      <th>Image</th>
      <th>Action</th>
    </tr>
  </thead>
  <%= form_tag user_map_path ,:method => 'get'  do %>
    <%  @readings.each do |reading| %>
      <tr>
	     <td><%= check_box_tag "reading_ids[]", reading.id %></td>        
       <%= render 'shared/readingss' ,:reading => reading %>

      </tr>
    <% end %>
    <%= submit_tag "Show map",:class => "btn" %>
  <% end %>
</table>  

<% end %>

