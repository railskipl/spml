<h1>Edit user</h1>
<%= form_for(@user) do |f| %>
<%= render 'shared/error_messages', :object => f.object %>

<div class="field">
<%= f.label :username %><br />
<%= f.text_field :username,:style => "height: 30px;" %>
</div>

<div class="field">
<%= f.label :password %><br />
<%= f.password_field :password,:style => "height: 30px;" %>
</div>
<div class="field">
<%= f.label :password_confirmation, "Confirmation" %><br />
<%= f.password_field :password_confirmation ,:style => "height: 30px;"%>
</div>
<div class="actions">
<%= f.submit "Update" %>
</div>
<% end %>
<div>

</div>
